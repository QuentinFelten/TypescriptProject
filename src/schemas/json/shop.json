{
  "$id": "shop.json",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": ["id", "name", "price", "stock"],
  "additionalProperties": false,
  "properties": {
    "list": { 
      "type":"array",
      "items": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "price": { "type": "number" }
      }
    }
  }
}
