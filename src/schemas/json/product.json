{
  "$id": "product.json",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": [
    "id",
    "description",
    "inStock",
    "unitPriceWithoutTax",
    "taxPercent"
  ],
  "additionalProperties": false,
  "properties": {
    "id": { "type": "number" },
    "name": { "type": "string" },
    "provider": { "type": "string" },
    "description": { "type": "string" },
    "stock": { "type": "string" },
    "price": { "type": "number" },
    "allergy": { "type": "string" }
  }
}
